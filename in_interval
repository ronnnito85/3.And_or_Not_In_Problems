a = input("Enter lower bound a:")
a = int(a)
b = input("Enter upper bound b:")
b = int(b)
x = input("Enter the number x:")
x = int(x)

if x == a:
    print("The number is equal to the lower bound of the interval %d and %d" % (a, b))
elif x == b:
    print("The number is equal to the upper bound of the interval %d and %d" % (a, b))
elif a < x and x < b:
    print("The number is in the interval %d and %d" % (a, b))
elif x < a:
    print("The number is outside the interval %d and %d" % (a, b))
elif x > b:
    print("The number is outside the interval %d and %d" % (a, b))
